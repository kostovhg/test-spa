<!-- Skills Section -->
<div class="container text-center">
    <h1 class="text-uppercase mb-0">{{nav.skills}}</h1>
    <hr class="star-light">
    <div class="skills">
       {{#if skills}}
    <ul class="list-group">
            {{#each skills}}
            {{>skill}}
            {{/each}}
            </ul>
        {{else}}
            <h3>This person is practically without any skills</h3>
       {{/if}}
    </div>
</div>